{
  "name": "PHP & Apache Development (Mutagen)",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "php",
  "workspaceFolder": "/var/www/html",
  "forwardPorts": [80, 8079, 3307],
  "remoteUser": "www-data",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
      "bash": {
        "path": "/bin/bash"
      }
    }
      },
        "extensions": [
            "neilbrayfield.php-docblocker",
            "xdebug.php-debug",
            "devsense.phptools-vscode",
            "devsense.composer-php-vscode",
            "xdebug.php-pack",
            "phproberto.vscode-php-getters-setters",
            "bmewburn.vscode-intelephense-client",
            "devsense.profiler-php-vscode",
            "reduxx.php-sort-imports",
            "swordev.phpstan"
        ]
    }
  },
  "initializeCommand": "mutagen-compose up -d",
  "shutdownAction": "none"
}
